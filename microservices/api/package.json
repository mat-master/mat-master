{
	"name": "api",
	"packageManager": "yarn@3.2.0",
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w --preserveWatchOutput",
		"dev": "yarn config set nmHoistingLimits 'workspaces' && yarn install && yarn config set nmHoistingLimits 'none' && rm -rf .aws-sam && overmind start",
		"aws-build": "rm -rf node_modules && sam build",
		"fix": "rm -rf node_modules && yarn config set nmHoistingLimits 'none' && yarn install"
	},
	"dependencies": {
		"@aws-sdk/client-ses": "^3.53.0",
		"bcryptjs": "^2.4.3",
		"dotenv": "^16.0.0",
		"jsonwebtoken": "^8.5.1",
		"pg": "^8.7.3",
		"stripe": "^8.205.0",
		"validator": "^13.7.0"
	},
	"devDependencies": {
		"@types/aws-lambda": "^8.10.92",
		"@types/bcryptjs": "^2.4.2",
		"@types/dotenv": "^8.2.0",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/pg": "^8.6.4",
		"@types/validator": "^13.7.1",
		"types": "^1.0.0",
		"typescript": "^4.0.5"
	}
}
